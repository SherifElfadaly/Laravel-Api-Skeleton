{
    "name": "api-skeleton/api-skeleton",
    "type": "library",
    "description": "skeleton app for developing restful apis",
    "keywords": [
        "ApiSkeleton",
        "ApiSkeleton"
    ],
    "homepage": "https://github.com/SherifElfadaly/ApiSkeleton",
    "license": "MIT",
    "authors": [
        {
            "name": ":Sherif Elfadaly",
            "email": "sh.elfadaly@gmail.com",
            "homepage": "",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/socialite": "2.0.*",
        "davibennun/laravel-push-notification": "dev-laravel5",
        "tymon/jwt-auth": "0.5.*",
        "caffeinated/modules": "^3.0",
        "predis/predis": "^1.0",
        "illuminate/support": "~5.1",
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "ApiSkeleton\\ApiSkeleton\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            ":ApiSkeleton\\ApiSkeleton\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "post-install-cmd": [
        "php artisan vendor:publish --force"
        ],
        "post-update-cmd": [
        "php artisan vendor:publish --force"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}

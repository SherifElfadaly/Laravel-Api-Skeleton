{
    "name": "api-skeleton/api-skeleton",
    "type": "library",
    "description": "skeleton app for developing restful apis",
    "keywords": [
        "ApiSkeleton",
        "ApiSkeleton"
    ],
    "homepage": "https://github.com/SherifElfadaly/ApiSkeleton",
    "license": "MIT",
    "authors": [
        {
            "name": ":Sherif Elfadaly",
            "email": "sh.elfadaly@gmail.com",
            "homepage": "",
            "role": "Developer"
        }
    ],
    "require": {
        "intervention/image": "^2.4",
        "optimus/api-consumer": "0.2.*",
        "enniel/laravel-fcm-notification-channel": "0.*",
        "phpdocumentor/reflection-docblock": "^4.1",
        "laravel/socialite": "3.0.*",
        "laravel/passport": "^4.0",
        "caffeinated/modules": "^4.3",
        "predis/predis": "^1.1",
        "illuminate/support": "~5.1",
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "ApiSkeleton\\ApiSkeleton\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            ":ApiSkeleton\\ApiSkeleton\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "post-install-cmd": [
        "php artisan vendor:publish --force"
        ],
        "post-update-cmd": [
        "php artisan vendor:publish --force"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "ApiSkeleton\\ApiSkeleton\\ApiSkeletonServiceProvider",
                "Optimus\\ApiConsumer\\Provider\\LaravelServiceProvider",
                "NotificationChannels\\FCM\\ServiceProvider",
                "Intervention\\Image\\ImageServiceProvider"
            ],
            "aliases": {
                "Core"        : "App\\Modules\\V1\\Core\\Facades\\Core",
                "ErrorHandler": "App\\Modules\\V1\\Core\\Facades\\ErrorHandler",
                "CoreConfig"  : "App\\Modules\\V1\\Core\\Facades\\CoreConfig",
                "Logging"     : "App\\Modules\\V1\\Core\\Facades\\Logging",
                "Media"       : "App\\Modules\\V1\\Core\\Facades\\Media",
                "Image"       : "Intervention\\Image\\Facades\\Image"
            }
        }
    }
}

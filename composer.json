{
    "name": "api-skeleton/api-skeleton",
    "type": "library",
    "description": "skeleton app for developing restful apis",
    "keywords": [
        "ApiSkeleton",
        "ApiSkeleton"
    ],
    "homepage": "https://github.com/SherifElfadaly/ApiSkeleton",
    "license": "MIT",
    "authors": [
        {
            "name": ":Sherif Elfadaly",
            "email": "sh.elfadaly@gmail.com",
            "homepage": "",
            "role": "Developer"
        }
    ],
    "require": {
        "itsgoingd/clockwork": "^1.14",
        "phpdocumentor/reflection-docblock": "^3.1",
        "laravel/socialite": "3.0.*",
        "davibennun/laravel-push-notification": "dev-laravel5",
        "tymon/jwt-auth": "0.5.*",
        "caffeinated/modules": "^4.0",
        "predis/predis": "^1.0",
        "illuminate/support": "~5.1",
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "ApiSkeleton\\ApiSkeleton\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            ":ApiSkeleton\\ApiSkeleton\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "post-install-cmd": [
        "php artisan vendor:publish --force"
        ],
        "post-update-cmd": [
        "php artisan vendor:publish --force"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "ApiSkeleton\\ApiSkeleton\\ApiSkeletonServiceProvider::class",
                "Caffeinated\\Modules\\ModulesServiceProvider::class",
                "Tymon\\JWTAuth\\Provider\\JWTAuthServiceProvider::class",
                "Davibennun\\LaravelPushNotification\\LaravelPushNotificationServiceProvider::class",
                "Laravel\\Socialite\\SocialiteServiceProvider::class",
                "Clockwork\\Support\\Laravel\\ClockworkServiceProvider::class",
            ],
            "aliases": {
                "Core"             : "App\\Modules\\V1\\Core\\Facades\\Core::class",
                "ErrorHandler"     : "App\\Modules\\V1\\Core\\Facades\\ErrorHandler::class",
                "CoreConfig"       : "App\\Modules\\V1\\Core\\Facades\\CoreConfig::class",
                "Logging"          : "App\\Modules\\V1\\Core\\Facades\\Logging::class",
                "Module"           : "Caffeinated\\Modules\\Facades\\Module::class",
                "JWTAuth"          : "Tymon\\JWTAuth\\Facades\\JWTAuth::class",
                "JWTFactory"       : "Tymon\\JWTAuth\\Facades\\JWTFactory::class",
                "PushNotification" : "Davibennun\\LaravelPushNotification\\Facades\\PushNotification::class",
                "Socialite"        : "Laravel\\Socialite\\Facades\\Socialite::class",
            }
        }
    }
}

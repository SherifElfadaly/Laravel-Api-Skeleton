{
    "name": ":SherifElfadaly/api_skeleton",
    "type": "library",
    "description": ":skeleton app for developing restful apis",
    "keywords": [
        ":vendor",
        ":package_name"
    ],
    "homepage": "https://github.com/SherifElfadaly/api_skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": ":Sherif Elfadaly",
            "email": ":sh.elfadaly@gmail.com",
            "homepage": ":author_website",
            "role": "Developer"
        }
    ],
    "require": {
        "caffeinated/modules": "~2.0",
        "predis/predis": "^1.0",
        "illuminate/support": "~5.1",
        "php" : "~5.5|~7.0",
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "SherifElfadaly\\api_skeleton\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            ":SherifElfadaly\\api_skeleton\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "post-install-cmd": [
        "...",
        "php artisan vendor:publish --force"
        ],
        "post-update-cmd": [
        "...",
        "php artisan vendor:publish --force"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
